{
    "preset": "laravel",
    "exclude": [
        "bootstrap",
        "config",
        "database",
        "public",
        "resources",
        "routes",
        "storage"
    ],
    "rules": {
        "phpdoc_order": {
            "order": [
                "property",
                "property-read",
                "property-write",
                "method",

                "param",
                "throws",
                "return"
            ]
        }
    }
}
